  stages {
    stage('build'){
      steps{
        echo "This is our first stage."
      }
    }
    stage('Test'){
      steps{
        echo "This is a test stage."
      }
    }
    stage('Deply'){
      steps{
        echo 'This is deploy stage.'
      }
    }
  }
}